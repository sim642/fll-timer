<!DOCTYPE html>
<html lang="en">
	<head>
		<title>Table</title>
		<link rel="stylesheet" href="css/bootstrap.min.css" />

		<link rel="stylesheet" href="css/table.css" />

		<script src="/js/jquery-2.1.3.min.js"></script>
		<script src="/js/bootstrap.min.js"></script>
		<script src="/js/jquery.fittext.js"></script>
		<script src="/socket.io/socket.io.js"></script>

		<script src="/js/table.js"></script>
	</head>
	<body>
		<table>
			<tr id="topbanner" class="minimize-row">
				<td colspan="2">
					<div class="container-fluid text-center">
						<div class="row">
							<div class="col-sm-4"><img class="sideimg" src="/img/City Shaper.png"></img></div>
							<div class="col-sm-4">
								<div class="container-fluid">
									<div class="row"><img id="midimg" src="/img/1_Robootika_logo_taustata.png"></img></div>
									<div id="matchtime" class="row"></div>
									<div id="roundname" class="row"></div>
								</div>
							</div>
							<div class="col-sm-4"><img class="sideimg" src="/img/City Shaper.png"></img></div>
						</div>
					</div>
				</td>
			</tr>
			<tr id="tableselect">
				<td colspan="2">
					<div class="container">
						<div class="row">
							<div class="col-sm-6 form-group">
								<select id="leftselect" class="form-control input-lg"></select>
							</div>
							<div class="col-sm-6 form-group">
								<select id="rightselect" class="form-control input-lg"></select>
							</div>
						</div>

						<div class="row">
							<div class="col-md-12 form-group text-center">
								<button id="select" class="btn btn-primary btn-lg">Select</button>
							</div>
						</div>
					</div>
				</td>
			</tr>
			<tr id="tablenames" class="minimize-row">
				<td id="lefttable" class="tablename"></td>
				<td id="righttable" class="tablename"></td>
			</tr>
			<tr id="teamnames">
				<td class="teamname"><span id="leftteam"></span></td>
				<td class="teamname"><span id="rightteam"></span></td>
			</tr>
			<tr id="tablelogos" class="minimize-row">
				<td colspan="2">
					<div class="container-fluid">
						<div class="row">
							<div id="leftlogo" class="col-sm-5 text-center vcenter">
								<img class="img-responsive center-block"></img>
							</div><!--
							--><div id="clocktime" class="col-sm-2 text-center vcenter"></div><!--
							--><div id="rightlogo" class="col-sm-5 text-center vcenter">
								<img class="img-responsive center-block"></img>
							</div>
						</div>
					</div>
				</td>
			</tr>
		</table>
	</body>
</html>
